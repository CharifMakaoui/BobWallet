{
  "name": "ShuffleLink",
  "version": "0.1.1",
  "description": "Trustless Bitcoin Privacy Wallet",
  "homepage": "https://bobwallet.github.io",
  "main": "./shufflelink/client/network.js",
  "scripts": {
    "server": "npm run babel-shufflelink && node ./shufflelink/server/index.js",
    "server-blindlink": "npm run babel-blindlink && node ./blindlink/server/server.js",
    "test": "npm run babel-shufflelink && tape ./shufflelink/tests/*.js",
    "test-blindlink": "npm run babel-blindlink && tape ./blindlink/tests/*.js",
    "babel": "npm run babel-blindlink && npm run babel-shufflelink",
    "babel-blindlink": "babel ./blindlink/client/*.js --presets babel-preset-es2015 --out-dir ./blindlink/dist/",
    "babel-shufflelink": "babel ./shufflelink/client/*.js --presets babel-preset-es2015 --out-dir ./shufflelink/dist/",
    "---pretest": "npm run babel",
    "prettier": "prettier --write --single-quote --trailing-comma es5 app/src/*.js app/src/**/*.js blindlink/**/*.js shufflelink/**/*.js",
    "lint": "node_modules/.bin/eslint blindlink shufflelink app/src",
    "prepublish": "npm run babel",
    "build": "npm install && npm run test && npm run build-app",
    "build-app": "cd app && npm install && npm run build",
    "build-bcoin": "sh ./scripts/build_bcoin",
    "dev": "cd app && npm install && npm start",
    "copy": "cd app && npm run copy",
    "release": "npm run prettier && npm run test && npm version patch"
  },
  "author": "Bob (bobwallet@protonmail.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.9.1",
    "prettier": "1.13.4",
    "tape": "4.9.0"
  },
  "dependencies": {
    "aes-js": "3.1.1",
    "axios": "0.18.0",
    "bcoin": "1.0.0-beta.15",
    "bitcoinjs-lib": "3.3.2",
    "bitcoinjs-message": "2.0.0",
    "bitcore-explorers": "1.0.1",
    "bitcore-lib": "0.15.0",
    "bitcore-mnemonic": "1.5.0",
    "blind-signatures": "1.0.5",
    "body-parser": "1.18.3",
    "build-app": "0.6.3",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "crypto-shuffle": "1.0.1",
    "express": "4.16.3",
    "greenlock-express": "2.1.6",
    "js-sha256": "0.9.0",
    "jsencrypt": "3.0.0-beta.1",
    "normalize-url": "3.0.1",
    "path": "0.12.7",
    "randombytes": "2.0.6",
    "simple-node-logger": "0.93.37",
    "socket.io": "2.1.1",
    "socket.io-client": "2.1.1"
  }
}
