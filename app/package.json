{
  "name": "bob-wallet",
  "version": "0.0.1",
  "homepage": "./",
  "description": "Bob Wallet",
  "author": "Bob (bobwallet@protonmail.com)",
  "license": "MIT",
  "dependencies": {
    "aes-js": "3.1.1",
    "axios": "0.18.0",
    "bignumber.js": "7.2.1",
    "bitcoinjs-lib": "3.3.2",
    "bitcoinjs-message": "2.0.0",
    "bitcore-explorers": "1.0.1",
    "blind-signatures": "1.0.5",
    "crypto-shuffle": "1.0.1",
    "file-saver": "1.3.3",
    "js-sha256": "0.9.0",
    "jsencrypt": "3.0.0-beta.1",
    "mobx": "3.6.1",
    "mobx-react": "4.4.2",
    "moment": "2.22.1",
    "qr-image": "3.2.0",
    "react": "16.4.0",
    "react-art": "16.4.0",
    "react-dom": "16.4.0",
    "react-native-web": "0.7.3",
    "react-scripts": "1.1.4",
    "socket.io-client": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "symlink": "rm -f ./node_modules/blindlink.json && ln -s ../../package.json ./node_modules/blindlink.json",
    "copy": "npm run copy-blindlink && npm run copy-shufflelink",
    "copy-blindlink": "npm run symlink && rm -rf ./src/blindlink && cp -rf ../blindlink/client ./src/blindlink",
    "copy-shufflelink": "npm run symlink && rm -rf ./src/shufflelink && cp -rf ../shufflelink/client ./src/shufflelink",
    "prebuild": "npm run copy-shufflelink",
    "postbuild": "npm run inline",
    "prestart": "npm run copy-shufflelink",
    "eject": "react-scripts eject",
    "inline": "html-inline -i ./build/index.html -o ../bobwallet.html -b ./build"
  },
  "devDependencies": {
    "html-inline": "1.2.0",
    "prop-types": "15.6.1"
  }
}
